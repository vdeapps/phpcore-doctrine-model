{
    "name": "vdeapps/phpcore-doctrine-model",
    "type": "library",
    "description": "Simple Doctrine Querybuilder",
    "keywords": [
        "vdeapps",
        "library",
        "database",
        "mysql",
        "oracle",
        "doctrine",
        "dbal"
    ],
    "homepage": "https://github.com/vdeapps/phpcore-doctrine-model",
    "license": "MIT",
    "authors": [
        {
            "name": "vdeApps",
            "email": "vdeapps@riseup.net",
            "homepage": "https://github.com/vdeapps/phpcore-doctrine-model",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "vdeapps@riseup.net",
        "issues": "https://github.com/vdeapps/phpcore-doctrine-model/issues"
    },
    "config": {
        "platform": {
            "php": "7.0.99"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "doctrine/dbal": "^2.5",
        "vdeapps/phpcore-dictionary": "^1.0",
        "vdeapps/phpcore-helper": "^1.0",
        "vdeapps/phpcore-chained-array": "^1.0",
        "vdeapps/phpcore-datetime": "^1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "vdeApps\\phpCore\\DoctrineModel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests/"
        }
    },
    "scripts": {
	"test" : "phpunit70",
        "phpcs" : "php vendor/squizlabs/php_codesniffer/bin/phpcs",
        "phpcbf" : "php vendor/squizlabs/php_codesniffer/bin/phpcbf",
        "cs-fix": "php-cs-fixer fix . --fixers=-psr0"
    }
}
